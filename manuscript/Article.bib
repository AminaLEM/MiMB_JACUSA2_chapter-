  issue           = {10},
  keywords        = {Adenosine, analogs & derivatives, genetics, metabolism; Animals; Gene Expression Regulation, Neoplastic; Humans; Methylation; Neoplasms, genetics, metabolism, pathology; RNA Processing, Post-Transcriptional; RNA, Messenger, genetics, metabolism; RNA, Neoplasm, genetics, metabolism},
  nlm-id          = {100962782},
  owner           = {NLM},
  pii             = {10.1038/s41580-019-0168-5},
  pmid            = {31520073},
  pubmodel        = {Print-Electronic},
  pubstate        = {ppublish},
