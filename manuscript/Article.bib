  keywords        = {Animals; Computational Biology, trends; Mutation, genetics; RNA, Messenger, genetics; Sequence Analysis, RNA, trends; 2′-O-ribose methylation; Nanopore; m(5)C; m(6)A; m(6)Am; mRNA modifications},
  nlm-id          = {8310903},
  owner           = {NLM},
  pii             = {S0167-7799(20)30166-9},
  pmc             = {PMC7326690},
  pmid            = {32620324},
  pubmodel        = {Print-Electronic},
  pubstate        = {ppublish},
  revised         = {2021-09-22},
